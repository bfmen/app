<view class="list data-v-69de20b2"><block a:if="{{title}}"><view data-event-opts="{{[['tap',[['headerClick',['$event']]]]]}}" class="header data-v-69de20b2" onTap="__e"><text class="title data-v-69de20b2">{{title}}</text><text class="more data-v-69de20b2">更多</text></view></block><view class="items data-v-69de20b2"><block a:for="{{dataList}}" a:for-item="item" a:for-index="index" a:key="vodid"><view data-event-opts="{{[['tap',[['itemClick',['$0'],[[['dataList','vodid',item.vodid]]]]]]]}}" class="{{(('item data-v-69de20b2')+' '+((isMin)?'itemMin':''))}}" onTap="__e"><block a:if="{{vodid==item.vodid&&videoUrl}}"><video class="{{(('video data-v-69de20b2')+' '+((isMin)?'videoMin':''))}}" autoplay title="{{item.title}}" poster="{{item.coverpic}}" picture-in-picture-mode="{{['push','pop']}}" src="{{videoUrl}}"></video></block><block a:else><image class="{{(('image _img data-v-69de20b2')+' '+((isMin)?'imagemMin':''))}}" src="{{item.coverpic}}" mode="aspectFit"></image></block><text class="data-v-69de20b2">{{item.title||item.catename}}</text><view class="tips data-v-69de20b2"><block a:if="{{item.isvip==1}}"><text class="isvip data-v-69de20b2">{{item.isvip==1?'vip':''}}</text></block><block a:if="{{item.definition>0}}"><text class="definition data-v-69de20b2">{{item.definition}}</text></block><block a:if="{{!isMin}}"><text class="updatetime data-v-69de20b2">{{item.updatetime}}</text></block><text class="upnum data-v-69de20b2">{{item.upnum}}</text><text class="scorenum data-v-69de20b2">{{item.scorenum}}</text><text class="duration data-v-69de20b2">{{item.duration}}</text></view></view></block></view></view>